{
  "name": "@datacraft-dsc/starfish-js",
  "version": "0.3.0",
  "description": "JavaScript Starfish client library",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "browser": "./dist/index.js",
  "type": "module",
  "scripts": {
    "test": "mocha",
    "clean": "rm -rf ./dist/ ./doc/ ./coverage ./.nyc_output",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts src/",
    "lint:fix": "eslint --fix --ignore-path .gitignore --ext .js,.ts src/",
    "build": "npm run clean && tsc",
    "build:docs": "typedoc --mode modules --out ./docs/ ./src/",
    "release": "npm run build && npm run build:docs && npm publish",
    "run": "ts-node"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datacraft-dsc/starfish-js.git"
  },
  "keywords": [],
  "author": "Datacraft <devops@datacraft.sg>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/datacraft-dsc/starfish-js/issues"
  },
  "homepage": "https://github.com/datacraft-dsc/starfish-js#README",
  "dependencies": {
    "@convex-dev/convex-api-js": "^0.2.7",
    "bl": "^5.0.0",
    "form-data": "^3.0.1",
    "js-base64": "^3.7.2",
    "mime-types": "^2.1.35",
    "node-fetch": "^3.2.4",
    "node-metamask": "^1.1.2",
    "query-string": "^7.1.1",
    "test": "^0.6.0",
    "url-join-ts": "^1.0.5"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^14.18.18",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "browserify": "^17.0.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify-es": "^2.0.0",
    "gulp-util": "^3.0.8",
    "js-yaml": "^3.14.1",
    "lodash.template": "^4.5.0",
    "mocha": "^6.2.3",
    "nyc": "^14.1.1",
    "prettier": "^2.6.2",
    "serve": "^11.3.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.14.1",
    "tsify": "^4.0.2",
    "tslint": "^5.20.1",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.10",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "yargs-parser": "^21.0.1"
  },
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
